n=int(input())
ol=list(map(int,input().split()))

nl=[]
pl=[]
for i in range(0,n):
    if ol[i]>0:
        pl.append(ol[i])
    else:
        nl.append(ol[i])
l=nl+pl
for i in l:
    print(i,end=' ')
    
