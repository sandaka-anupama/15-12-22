n=int(input())
ol=list(map(int,input().split()))
el=[]
oddl=[]
for i in range(0,n):
    if ol[i]%2==0:
        el.append(ol[i])
    else:
        oddl.append(ol[i])
l=el+oddl
for i in l:
    print(i,end=' ')
